{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "**/Autodesk*.csproj"
          ]
        }
      ],
      "dest": "api",
      "namespaceLayout": "nested"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**/*.*"
        ]

      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Autodesk Platform SDK Documentation",
      "_appTitle": "Autodesk Platform SDK Documentation",
      "_enableSearch": true,
      "pdf": false
    }
  }
}

